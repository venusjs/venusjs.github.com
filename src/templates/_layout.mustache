<!DOCTYPE html>
<!--[if lt IE 7]><html class="no-js ie ie6 lte9 lte8 lte7" lang="{{ site.lang }}"><![endif]-->
<!--[if IE 7]><html class="no-js ie ie7 lte9 lte8 lte7" lang="{{ site.lang }}"><![endif]-->
<!--[if IE 8]><html class="no-js ie ie8 lte9 lte8" lang="{{ site.lang }}"><![endif]-->
<!--[if IE 9]><html class="no-js ie ie9 lte9" lang="{{ site.lang }}"><![endif]-->
<!--[if gt IE 9]><!--><html class="no-js" lang="en"><!--<![endif]-->
<head>
  <link href="http://fonts.googleapis.com/css?family=Cabin:400,500,600,700" rel="stylesheet" type="text/css">
  <!-- {{#stylesheet_bundle}}/css/all.css{{/stylesheet_bundle}} -->
  <link href="/css/main.css" rel="stylesheet" type="text/css">
  <link href="/css/venus.css" rel="stylesheet" type="text/css">
</head>
<body>
  <header id="global-header">
    <div>
      <a class="logo" href="/">
        <img src="/images/logos/venus.png" alt="{{ site.title }}" />
      </a>
      <nav>
        <ul>
          {{#navbar}}
          <li><a href="{{href}}">{{label}}</a></li>
          {{/navbar}}
        </ul>
      </nav>
    </div>
  </header>
  <div id="main" role="main">
    <article>
      <h1>{{{page.header}}}</h1>
      <hr>
      {{{ content }}}
  </article>
</div>
<footer id="global-footer">
  <div>
    <p class="copyright">
      LinkedIn Corporation &copy; 2013 
    </p>
      <ul>
        {{#footerlinks}}
        <li><a href="{{href}}">{{label}}</a></li>
        {{/footerlinks}}
      </ul>
    <p class="social-actions">
      <iframe width="112px" scrolling="0" height="20px" frameborder="0" allowtransparency="true" src="http://ghbtns.com/github-btn.html?user=linkedin&amp;repo=venus.js&amp;type=watch&amp;count=true"></iframe>
      <iframe width="98px" scrolling="0" height="20px" frameborder="0" allowtransparency="true" src="http://ghbtns.com/github-btn.html?user=linkedin&amp;repo=venus&amp;type=fork&amp;count=true"></iframe>
    </p>
  </div>
</footer>
  <script type="text/javascript" src="/js/jquery.js"></script>
  <script type="text/javascript" src="/js/venus.js"></script>
  <script type="text/javascript">

    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-36360221-1']);
    _gaq.push(['_trackPageview']);

    (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();

  </script>
</body>
</html>
